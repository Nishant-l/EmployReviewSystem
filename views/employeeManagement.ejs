<h1>Create new Employee</h1>
<form action="/employeeSignUp-form" method="post">
    <input type="email" name="email" id="" placeholder="email" required>
    <input type="password" name="password" id="" placeholder="password" required>
    <input type="text" name="name" id="" placeholder="name" required>
    <input type="submit">
</form>

<h1>List Of All Employees</h1>

<table style="width: 40%;">
    <tr>
        <th>Name of employee</th>
        <th>Update Employee details and review</th>
        <th>Delete employee</th>
    </tr>
    <% for(emp of AllEmployee){%>
        <tr>
            <td><h3 style="display: inline;"><%= emp.name %></h3></td>
            <td><a href="/admin/employeeManagement/Update-emloyee-profile/<%= emp.id %>">✏️</a></td>
            <td><a href="">🗑</a></td>
        </tr>
    <%}%>
</table>